    "generic.attribute:rust": {
      "foreground": "#000001"
    },
    "heading": {
      "foreground": "#000003"
    },
    "text.emph": {
      "foreground": "#000003",
      "fontStyle": "italic"
    },
    "text.strong": {
      "foreground": "#000003",
      "fontStyle": "bold"
    },
    "text.math": {
      "foreground": "#000010"
    },
    "pol": {
      "foreground": "#000010"
