      "settings": {
        "foreground": "#000001",
        "fontStyle": ""
      }
    },
    {
      "name": "Parentheses, Brackets, Braces",
      "scope": "punctuation",
      "settings": {
        "foreground": "#000011",
        "fontStyle": ""
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment"
